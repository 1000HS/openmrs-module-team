<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
   <class name="org.openmrs.module.teammodule.TeamHierarchy" table="team_role">
        <id column="team_role_id" name="teamHierarchyId" type="int">
            <generator class="native"/>
        </id> 
	    <property name="name" column="name" type="java.lang.String"/>
		<property name="ownsTeam" column="owns_Team" type="java.lang.Boolean"/>
		<many-to-one name="reportTo" column="report_to" class="org.openmrs.module.teammodule.TeamHierarchy" />
		<property name="uuid" column="uuid" type="java.lang.String"/>
		

	</class>
</hibernate-mapping>