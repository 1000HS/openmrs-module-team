<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
   <class name="org.openmrs.module.teammodule.TeamLog" table="team_log">
        <id column="team_log_id" name="logId" type="int">
            <generator class="native"/>
        </id> 
	    <many-to-one name="team" column="team" class="org.openmrs.module.teammodule.Team"/>

	    <property name="action" column="action" type="java.lang.String"/>
		<property name="dataNew" column="data_new" type="java.lang.String"/>
		<property name="log" column="log" type="java.lang.String"/>

		<property name="uuid" column="uuid" type="java.lang.String"/>
	</class>
</hibernate-mapping>